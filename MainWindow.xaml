<Window x:Class="ZModLauncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Height="869" Width="1546"
        MinHeight="830"
        MinWidth="1024"
        WindowStyle="SingleBorderWindow"
        WindowStartupLocation="CenterScreen"
        Style="{StaticResource {x:Type Window}}" Title="{Binding ExecutableName, ElementName=This}"
        FontSize="0.01"
        SizeToContent="Manual"
        Name="This" Initialized="This_Initialized">
    <Grid>
        <Frame
            x:Name="MainFrame"
            Source="Pages/LoadingPage.xaml"
            NavigationUIVisibility="Hidden" />

    </Grid>
</Window>